{
  "expo": {
    "name": "Ecg12d",
    "slug": "ecg12d",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icone.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vhiculemedia.testecg",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la caméra pour numériser le code-barres des cartes."
      }
    },
    "android": {
      "package": "com.vhiculemedia.coffretprestige",
      "versionCode": 18,
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}