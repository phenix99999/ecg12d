{
  "expo": {
    "name": "Coffrets Prestige",
    "slug": "coffrets-prestige",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#231F20"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vhiculemedia.cpbeta",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application utilise la caméra pour numériser le code-barres des cartes."
      }
    },
    "android": {
      "package": "com.vhiculemedia.coffretprestige",
      "versionCode": 5,
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}